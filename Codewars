2 months agoRefactorDiscuss
def to_weird_case(string):
    blank = ""
    for word in string.split(" "):
        count = 0
        for letter in word:
            if count % 2 != 0:
                blank += letter.lower()
            else:
                blank += letter.upper()
            count += 1
        blank += " "
    return blank.strip()
            
    
            
    
            
            
            


    
