def alphabet_position(text):
    ord = list("abcdefghijklmnopqrstuvwxyz")
    return " ".join([str(ord.index(x.lower()) + 1) for x in text if x.lower() in "abcdefghijklmnopqrstuvwxyz"])
    

    
    
    
