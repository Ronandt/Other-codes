def perimeter(n):
    #generate all the numbers (using finbonacci number), create a list , use "sum"
    listed = [0, 1]
    for i in range(n):
        listed.append(listed[-1] + listed[-2])
        
    return sum(listed) * 4
        
        
        
        
        
        
